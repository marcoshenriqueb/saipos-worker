NODE_ENV=development

# Postgres
DATABASE_URL=postgres://postgres:postgres@localhost:5432/saipos

# Worker mode: idle | ingest | normalize
WORKER_MODE=ingest

# Main loop interval (ms)
POLL_INTERVAL_MS=60000

# Saipos Data API
SAIPOS_DATA_API_URL=https://data.saipos.io
SAIPOS_DATA_TOKEN=replace_with_jwt_token

# Ingest window (days back from now - 26h)
INGEST_DAYS_BACK=2

# Normalizer batch size per cycle
NORMALIZER_BATCH_SIZE=100
